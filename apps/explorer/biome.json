{
  "$schema": "https://biomejs.dev/schemas/1.9.0/schema.json",
  "organizeImports": {
    "enabled": false
  },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "complexity": {
        "useArrowFunction": "off"
      }
    }
  },
  "formatter": {
    "enabled": false
  },
  "files": {
    "ignore": [".next/", "node_modules/"]
  }
}
