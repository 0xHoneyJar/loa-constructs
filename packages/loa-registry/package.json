{
  "name": "@loa-constructs/cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "CLI plugin for Loa Constructs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@loa-constructs/shared": "workspace:*",
    "conf": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.6.0",
    "tsup": "^8.3.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  }
}
